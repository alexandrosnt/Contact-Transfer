{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "./out/renderer",
    "types": [],
    "strict": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "src/renderer/src/**/*.ts",
    "src/renderer/src/**/*.svelte",
    "src/renderer/env.d.ts"
  ]
}
